[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=django-deploy
Group=www-data
WorkingDirectory=/home/django-deploy/get_data
ExecStart=/home/django-deploy/myenv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/django-deploy/quantcore1/get_data.sock get_data.wsgi:application

[Install]
WantedBy=multi-user.target
